# .gitignore for a React project

# Node modules
node_modules/

# Build directories
build/
dist/

# Environment variables
#.env
#.env.local
#.env.development.local
#.env.test.local
#.env.production.local

# npm log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# IDEs and text editor settings
.vscode/
.idea/

# OS-specific files
.DS_Store
Thumbs.db

# Logs
logs/
*.log

# Coverage reports from testing
coverage/

# React Native specific
.expo/
.expo-shared/

# Storybook specific
storybook-static/

# Test outputs
jest-test-results.json

# SASS / SCSS Cache
.sass-cache/

# Temporary files generated by the system
*.swp
*.swo
